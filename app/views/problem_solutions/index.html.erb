<div align="right">
  <%= generate_common_links({index_path: problem_solutions_path(request.parameters.merge({format: :xls})), create_path: new_problem_solution_path, object: @problem_solutions, create_text: "Add New Problem Solution" }) %>
</div>
<div align="left">
  <%= generate_search_form({index_path: problem_solutions_path, placeholder: params[:search] ? params[:search] : "Search Problem Solutions" }) %>
</div>
<fieldset>
  <legend> Problem Solutions List</legend>
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Problem Solution</th>
        <th data-sorter="false">Actions</th>
      </tr>
    </thead>
    <tbody>
      <%= render @problem_solutions %>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="2"><%= pluralize(@problem_solutions.size, 'problem solution') %></td>
      </tr>
    </tfoot>
  </table>
  <%= will_paginate @problem_solutions %>
</fieldset>